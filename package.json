{
  "name": "goodreads-chrome-extension",
  "version": "0.0.0-development",
  "description": "Goodreads Chrome Extension",
  "main": "index.js",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "build:dev": "NODE_ENV=development node_modules/.bin/webpack --config configs/webpack.config.development.js",
    "build:prod": "NODE_ENV=production node_modules/.bin/webpack --config configs/webpack.config.production.js",
    "clear-cache": "rm -rf ./node_modules/.cache/hard-source",
    "clear-everything": "npm run clear-cache && ./node_modules/.bin/rimraf ./node_modules",
    "codecov": "codecov",
    "fresh-start": "npm run clear-everything && npm install",
    "lint": "./node_modules/.bin/eslint source/",
    "semantic-release": "semantic-release",
    "test": "./node_modules/.bin/jest --coverage --passWithNoTests",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "goodreads",
    "chrome"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/goodreads-chrome-extension.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "chrome-webstore-upload-cli": "^1.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-css-chunks-webpack-plugin": "^4.0.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "husky": "^1.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "semantic-release": "^15.12.1",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "travis-deploy-once": "^5.0.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-build-notifier": "^0.1.30",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-chrome-extension-reloader": "^1.2.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "axios": "^0.21.3",
    "dom-loaded": "^1.0.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.85.0",
    "webextension-polyfill": "^0.4.0"
  }
}
